{
  "version": "1.0.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "on-release": "tsdx build",
    "release": "npm run build && npm publish --access public"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@pbbbl/stringify",
  "author": "Tyler Grow",
  "module": "dist/serialize.esm.js",
  "size-limit": [
    {
      "path": "dist/serialize.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/serialize.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.8",
    "husky": "^7.0.4",
    "size-limit": "^7.0.8",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.0",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "@pbbbl/clean-deep": "^1.1.0",
    "@pbbbl/is": "^0.1.0",
    "copy-anything": "^3.0.2",
    "is-json": "^2.0.1"
  }
}
